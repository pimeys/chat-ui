        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "${datasource}"
          },
          "format": 0,
          "rawSql": "WITH token_deltas AS (\n    SELECT\n        Attributes['client.id'] as client_id,\n        Attributes['gen_ai.request.model'] as model,\n        MetricName,\n        Value - lagInFrame(Value, 1, Value) OVER (PARTITION BY MetricName, Attributes['gen_ai.request.model'], Attributes['client.id'] ORDER BY TimeUnix) as delta\n    FROM otel.otel_metrics_sum\n    WHERE\n        MetricName IN ('gen_ai.client.input.token.usage', 'gen_ai.client.output.token.usage')\n        AND ServiceName = 'nexus-docker'\n        AND $__timeFilter(TimeUnix)\n)\nSELECT\n    client_id as \"Client ID\",\n    sum(if(MetricName = 'gen_ai.client.input.token.usage' AND delta >= 0, delta, 0)) as \"Input Tokens\",\n    sum(if(MetricName = 'gen_ai.client.output.token.usage' AND delta >= 0, delta, 0)) as \"Output Tokens\",\n    sum(if(delta >= 0, delta, 0)) as \"Total Tokens\"\nFROM token_deltas\nWHERE client_id IS NOT NULL AND client_id != ''\nGROUP BY client_id\nHAVING \"Total Tokens\" > 0\nORDER BY \"Total Tokens\" DESC",
          "refId": "A"
        }
      ],
      "title": "Token Usage by Client ID",
      "type": "table"
