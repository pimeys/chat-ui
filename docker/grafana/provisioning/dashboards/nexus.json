      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "${datasource}"
          },
          "format": 0,
          "rawSql": "WITH unique_values AS (\n    SELECT\n        min(TimeUnix) as first_seen,\n        Attributes['gen_ai.request.model'] as model,\n        Sum/Count as latency,\n        Count\n    FROM otel.otel_metrics_histogram\n    WHERE\n        MetricName = 'gen_ai.client.time_to_first_token'\n        AND ServiceName = 'nexus-docker'\n        AND $__timeFilter(TimeUnix)\n        AND Attributes['gen_ai.request.model'] != ''\n        AND Count > 0\n    GROUP BY model, latency, Count\n),\ntime_windows AS (\n    SELECT\n        toStartOfInterval(first_seen, INTERVAL 1 minute) as time,\n        model,\n        avg(latency) as value\n    FROM unique_values\n    GROUP BY time, model\n)\nSELECT\n    time,\n    model,\n    value\nFROM time_windows\nORDER BY time, model",
          "refId": "A"
        }
