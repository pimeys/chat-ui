          "format": 0,
          "rawSql": "WITH combined AS (\n    SELECT\n        Attributes['gen_ai.request.model'] as model,\n        MetricName,\n        Attributes['client.id'] as client_id,\n        max(Value) - min(Value) as tokens_in_window\n    FROM otel.otel_metrics_sum\n    WHERE\n        MetricName IN ('gen_ai.client.input.token.usage', 'gen_ai.client.output.token.usage')\n        AND ServiceName = 'nexus-docker'\n        AND $__timeFilter(TimeUnix)\n    GROUP BY model, MetricName, client_id\n)\nSELECT\n    model as \"Model\",\n    sum(if(MetricName = 'gen_ai.client.input.token.usage', tokens_in_window, 0)) as \"Input Tokens\",\n    sum(if(MetricName = 'gen_ai.client.output.token.usage', tokens_in_window, 0)) as \"Output Tokens\"\nFROM combined\nWHERE model != ''\nGROUP BY model\nORDER BY (\"Input Tokens\" + \"Output Tokens\") DESC",
